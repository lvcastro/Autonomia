<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A3EBB1"
    tools:context=".CadMec">

    <EditText
        android:id="@+id/txtEstadoMec"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite o estado"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rua:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="204dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="61dp"
        android:layout_height="15dp"
        android:text="Numero:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="239dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite o numero"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="229dp" />

    <RatingBar
        android:id="@+id/ratingBar"
        style="@android:style/Widget.DeviceDefault.Light.RatingBar.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rating="4"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="308dp" />

    <Button
        android:id="@+id/btnCancCadMec"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:background="@drawable/ic_launcher_background"
        android:text="CANCELAR"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="434dp" />

    <EditText
        android:id="@+id/txtRuaMec"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite a rua"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="75dp"
        android:layout_height="22dp"
        android:text="Avaliação:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="270dp" />

    <EditText
        android:id="@+id/txtCidadeMec"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite a cidade"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="131dp" />

    <Button
        android:id="@+id/btnEnviarCadMec"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:background="@drawable/ic_launcher_background"
        android:text="Enviar"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="434dp" />

    <EditText
        android:id="@+id/txtBairroMec"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite o bairro"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="166dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Estado:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="106dp" />

    <EditText
        android:id="@+id/txtNomeMec"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Digite o nome"
        android:inputType="textPersonName"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="60dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bairro:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="176dp" />

    <TextView
        android:id="@+id/lblCadMec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro de Mecanico"
        android:textSize="28sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cidade:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="70dp" />

</androidx.constraintlayout.widget.ConstraintLayout>